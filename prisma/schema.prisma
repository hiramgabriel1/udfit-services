generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id       String @id @default(uuid())
  username String
  email    String
  password String
  weight   Int
  height   Int
  age      Int
  gender   String
  role     String 
}